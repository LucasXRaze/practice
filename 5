     printf(" hello Lucas "); 
     printf(" my height is %d m %d cm \n",1,62);
     printf(" this building is %5d cm \n", 950);
     printf(" this building is %5d cm \n", 4520);
     printf(" this building is %5d cm \n", 67850);
     printf(" this building is %5d cm \n", 5646210);
     printf(" this building is %5d cm \n", 96512510);
     printf(" pi = %1.10f \n ", 3.1415926535897932384626433832795);
}
