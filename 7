#include<stdio.h>
int main() {
    int a, b, product;
    a = 14;
   
    b = 7;

 
    // Calculating product
    product = a + b;

    printf("a + b = %d \n", product);
   
     product = a - b;

    printf("product = %d \n", product);
   
     product = a / b;

    printf("Product = %d /n", product);

    product = a * b;

    printf("Product = %d /n", product);
   
    product = a % b;

    printf("Product = %d /n ", product);
   
    /*
   
    ++x
x++
--x
x--

x = 16
x >> 1
x >> 2
x << 1

*/
printf("a = %d \n ", a);
++a;
printf("++a = %d \n ", a);
printf("b = %d \n ", b);
--b;
printf("--b = %d \n ", b);
printf("Product = %d \n ", b);
a--;
printf("Product = %d \n ", a);
a= 16;  
 printf("Product = %d \n ", a);
   
  a >>1  ;
  printf("Product = %d \n ", a);  
  a >>2  ;
   printf("Product = %d \n ", a);
  b <<1  ;
 printf("Product = %d \n ", b);  
   
   
   
}
